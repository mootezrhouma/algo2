ALGORITHM sumofarrays
VAR
    
    arr=set1 : ARRAY_OF INTEGER[4];
    arr=set2 : ARRAY_OF INTEGER[5];
    i=INTEGER
    sum=INTEGER

BEGIN
    sum=0;
    i=0;
    set1=[3,1,7,9];
   write('print set1')
   read('set1[i]')
   set2=[2,4,1,9,3]
   write('print set2')
   read('set2[i]')
   FOR i FROM 0 TO arr.lenght-1 STEP 1 DO
   IF (set1[i]<>7 ) THEN
    set1[3,1,9]   END_IF
    FOR i FROM 0 TO arr.lenght-1 STEP 1  DO
        IF (set2[i] <> 4,2) THEN
            set2[1,9,3]
        END_IF
       
        ELSE
            sum=7+4+2
        END_IF
    END_FOR
   END_FOR
   
    END_FOR

END
