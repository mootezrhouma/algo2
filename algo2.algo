    hash2.insert(9,"nine")
    hash2.insert(3,"three")
    write('hash2')
    read('hash2')
    FOR i FROM 0 TO hash1.lenght-1 STEP 1 DO

IF (hash1.insert<>7)) THEN
    hash1.insert(3,"three")
    hash1.insert(1,"one")
    hash1.insert(9,"nine")
    sum=3+1+9
  
        
    END_IF
         FOR i FROM 0 TO hash2.lenght-1 STEP 1  DO
    IF (hash2.insert<>2,4) THEN
        hash1.insert(1,"one")
        hash1.insert(9,"nine")
        hash1.insert(3,"three")
        sum=1+3+9
    END_FOR
END_IF       
ELSE
sum=7+4+2 
